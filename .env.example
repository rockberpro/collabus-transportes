# MongoDB Configuration for Nuxt Application
MONGODB_URI=mongodb://your_user:your_password@mongodb:27017/collabus?authSource=collabus
MONGODB_DB_NAME=collabus
MONGODB_AUTH_SOURCE=collabus

# Application
NODE_ENV=development

# Supabase (if using)
SUPABASE_URL=your_supabase_url
SUPABASE_KEY=your_supabase_anon_key

# Gmail SMTP Configuration
GMAIL_USER=your_email@gmail.com
GMAIL_PASS=your_app_password
